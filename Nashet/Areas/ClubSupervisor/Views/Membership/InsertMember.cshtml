@model MembershipViewModel
<section class="page_wrap">
    <div class="page_content">

        <div class="block mb-4">
            <div class="block-header border-bottom px-4 py-4">
                <h5 class="m-0">إضافة عضو </h5>
            </div>
            <div class="block-body py-5">
                <div class="row justify-content-center">
                    <div class="col-md-8">
                        <form asp-action="InsertMember">
                            <div class="row">
                                <!-- START ITEM -->
                                <div class="col-md-6">
                                    <label for="" class="mb-1"> الفريق </label>
                                    <select asp-for="TeamId" class="form-control">
                                        <option value=""> اختر الفريق</option>
                                        @foreach (TeamViewModel team in ViewBag.Team)
                                        {
                                            <option value="@team.TeamId">@team.TeamNameAR</option>
                                        }
                                    </select>
                                    <span style="color:red" asp-validation-for="TeamId"></span>
                                </div><!-- END ITEM -->
                            </div>
                            <div class="row">
                                <!-- START ITEM -->
                                <div class="col-md-6">
                                @*     <!-- START ITEM -->
                                    <label asp-for="StudentNameAr" class="mb-1"></label>
                                    <input asp-for="StudentNameAr" type="text" class="form-control">
                                    <span style="color:red" asp-validation-for="StudentNameAr"></span>
                                </div><!-- END ITEM --> *@
                                <!-- START ITEM -->
                                <div class="col-md-6">
                                        <label asp-for="StudentId" class="mb-1"></label>
                                    <input asp-for="StudentId" type="text" class="form-control">
                                        <span style="color:red" asp-validation-for="StudentId"></span>
                                </div><!-- END ITEM -->

                                <div class="d-flex justify-content-end">
                                    <button type="submit" class="btn btn-primary px-5" title="save">حفظ</button>
                                </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section><!-- End Page Wrap -->